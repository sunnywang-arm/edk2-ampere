                                    // <Led:6-7.Led_action:4-5.Ampere_HP_CMD:0-3>
                                    // Ampere_HP_CMD: LED_CMD = 4, HP_CHG_CMD = 3
            Store(0x6800027B, OUTV) // <MsgType(0x6)-Hdlr(0x8).Resv.action.dev.segment>
                                    // action: Insert = 1 & Remove = 2
